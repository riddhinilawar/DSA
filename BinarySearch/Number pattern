pattern 1 

1 ->1
2 ->2
3 ->3
4 ->5
5 ->7..

while(low<=high){
  mid=..
  if(helper(mid)>=givenVal){
     ans=mid;
     high=mid-1;
  }
  else{
    low=mid+1;
  }
}

